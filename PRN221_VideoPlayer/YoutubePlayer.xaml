<Window x:Class="PRN221_VideoPlayer.YoutubePlayer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRN221_VideoPlayer"
        mc:Ignorable="d"
        Title="YoutubePlayer" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtUrl" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="700" Height="40"/>
        <Button x:Name="btnGo" Content="Go" HorizontalAlignment="Left" Height="40" Margin="715,10,0,0" VerticalAlignment="Top" Width="75"/>
        <WebBrowser Margin="0,55,0,0"/>

    </Grid>
</Window>
